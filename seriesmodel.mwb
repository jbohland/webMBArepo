ALTER TABLE `seriesbrowser_sectionnote` ADD CONSTRAINT `section_id_refs_id_6d179e29` FOREIGN KEY (`section_id`) REFERENCES `seriesbrowser_section` (`id`);
ALTER TABLE `seriesbrowser_sectionnote` ADD CONSTRAINT `updater_id_refs_id_b1dda67b` FOREIGN KEY (`updater_id`) REFERENCES `seriesbrowser_updater` (`id`);
