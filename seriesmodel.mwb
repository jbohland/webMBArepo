ALTER TABLE `seriesbrowser_seriesnote` ADD CONSTRAINT `updater_id_refs_id_459b8302` FOREIGN KEY (`updater_id`) REFERENCES `seriesbrowser_updater` (`id`);
CREATE TABLE `seriesbrowser_sectionnote` (
    `id` integer AUTO_INCREMENT NOT NULL PRIMARY KEY,
